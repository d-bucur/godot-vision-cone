; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "Node2D",
"class": &"VisionCone2D",
"language": &"GDScript",
"path": "res://addons/vision_cone_2d/vision_cone_2d.gd"
}]
_global_script_class_icons={
"VisionCone2D": ""
}

[application]

config/name="vision-cone"
run/main_scene="res://addons/vision_cone_2d/examples/example.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1200
window/size/viewport_height=800
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/vision_cone_2d/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
